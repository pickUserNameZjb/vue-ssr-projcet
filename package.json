{
  "name": "h5-ssr-webpack",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf ./public && rimraf ./server-build",
    "test": "rimraf ./public && cross-env NODE_ENV=production webpack --config ./build/webpack.config.base.js",
    "build:client": "cross-env NODE_ENV=production webpack --config ./build/webpack.config.client.js",
    "build:server": "cross-env NODE_ENV=production webpack --config ./build/webpack.config.server.js",
    "build": "npm run clean && npm run build:client && npm run build:server",
    "dev:server": "cross-env NODE_ENV=development nodemon ./src/server/server.js",
    "dev:client": "cross-env NODE_ENV=development webpack-dev-server --config ./build/webpack.config.client.js",
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:server\"",
    "start": "cross-env NODE_ENV=production node ./src/server/server.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^9.6.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-1": "^6.24.1",
    "chalk": "^2.4.2",
    "concurrently": "^4.1.1",
    "cross-env": "^5.2.0",
    "css-loader": "^3.0.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^4.0.0",
    "html-webpack-plugin": "^3.2.0",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "mini-css-extract-plugin": "^0.7.0",
    "nodemon": "^1.19.1",
    "postcss-loader": "^3.0.0",
    "rimraf": "^2.6.3",
    "style-loader": "^0.23.1",
    "url-loader": "^2.0.1",
    "vue-loader": "^15.7.0",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.35.2",
    "webpack-cli": "^3.3.5",
    "webpack-dev-server": "^3.7.2",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "ejs": "^2.6.2",
    "koa": "^2.7.0",
    "koa-router": "^7.4.0",
    "koa-send": "^5.0.0",
    "loadsh": "0.0.4",
    "memory-fs": "^0.4.1",
    "vue": "^2.6.10",
    "vue-router": "^3.0.7",
    "vue-server-renderer": "^2.6.10"
  }
